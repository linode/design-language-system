{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
  },
  "include": [
    "style-dictionary/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
